	int int_evento;			/*Numero de evento*/
	int nro_cuenta;			/*Numero cuenta*/
	int monto			/*Saldo*/
	char char_mensaje[1024];	/*mensajse*/
};

typedef struct clientes cliente;
struct clientes
{
