CC=gcc-4.1

CFLAGS = -Wall -O4 -std=gnu89 -pedantic-errors
HEADERS = -I./
all : carga reserva

carga: carga.c carga.h gestorArchivos.c funciones.c semaforo.c clave.c

	$(CC) -o carga carga.c gestorArchivos.c funciones.c semaforo.c clave.c $(CFLAGS) $(HEADERS)

reserva: reserva.c reserva.c gestorArchivos.c funciones.c semaforo.c clave.c

	$(CC) -o reserva reserva.c gestorArchivos.c funciones.c semaforo.c clave.c $(CFLAGS) $(HEADERS)
 

clean:
	rm -rf *o carga reserva
	
	
	
